class DLLNode:
    def __init__(self, val):
        self.data = val
        self.next = None
        self.prev = None


class Solution:
    def reverseDLL(self, head):
        if not head or not head.next:
            return head
        current=head
        temp=None
        while current:
            temp=current.prev
            current.prev = current.next
            current.next=temp
            current=current.prev
        
        if temp:
            head = temp.prev

        return head
