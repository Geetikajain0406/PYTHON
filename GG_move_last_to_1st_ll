from typing import Optional

class Node:
    def __init__(self,x):
        self.data=x
        self.next=None



class Solution:
    def moveToFront(self, head : Optional['Node']) -> Optional['Node']:
        if head is None or head.next is None:
            return head

        current=head
        prev=None
        while current.next:
            prev=current
            current=current.next
            
        prev.next=None
        current.next=head
        head=current
        return head
       
